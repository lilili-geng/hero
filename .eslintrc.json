{
  "extends": "next/core-web-vitals",
  "plugins": [
    "simple-import-sort",
    "unused-imports"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  },
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "unused-imports/no-unused-imports": "warn",
    "import/no-anonymous-default-export": [
      "warn",
      {
        "allowObject": true
      }
    ]
  }
}